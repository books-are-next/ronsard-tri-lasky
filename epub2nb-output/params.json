    "chapters": [
      {
        "titleSuggest": "",
        "subtitleSuggest": "",
        "filename": "tri_lasky_001.md",
        "xhtml": "tri_lasky_001.xhtml"
      },
      {
        "titleSuggest": "",
        "subtitleSuggest": "",
        "filename": "tri_lasky_002.md",
        "xhtml": "tri_lasky_002.xhtml"
      },
      {
        "titleSuggest": "",
        "subtitleSuggest": "",
        "filename": "tri_lasky_003.md",
        "xhtml": "tri_lasky_003.xhtml"
      },
      {
        "titleSuggest": "",
        "subtitleSuggest": "",
        "filename": "tri_lasky_004.md",
        "xhtml": "tri_lasky_004.xhtml"
      },
      {
        "titleSuggest": "KASANDRA",
        "subtitleSuggest": "",
        "filename": "tri_lasky_005.md",
        "xhtml": "tri_lasky_005.xhtml"
      },
      {
        "titleSuggest": "Doufám a zoufám, nemám slov a prosím,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_006.md",
        "xhtml": "tri_lasky_006.xhtml"
      },
      {
        "titleSuggest": "Dej mně mír, lásko, poruč už klid zbraní,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_007.md",
        "xhtml": "tri_lasky_007.xhtml"
      },
      {
        "titleSuggest": "Vypálím na svět celé svoje hoře",
        "subtitleSuggest": "",
        "filename": "tri_lasky_008.md",
        "xhtml": "tri_lasky_008.xhtml"
      },
      {
        "titleSuggest": "Chtěl bych se zvolna jako hustý proud",
        "subtitleSuggest": "",
        "filename": "tri_lasky_009.md",
        "xhtml": "tri_lasky_009.xhtml"
      },
      {
        "titleSuggest": "Má milá, pojďme podívat se,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_010.md",
        "xhtml": "tri_lasky_010.xhtml"
      },
      {
        "titleSuggest": "Tisíckrát myslet tutéž myšlenku,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_011.md",
        "xhtml": "tri_lasky_011.xhtml"
      },
      {
        "titleSuggest": "To, že smím svírat v pažích den co den",
        "subtitleSuggest": "",
        "filename": "tri_lasky_012.md",
        "xhtml": "tri_lasky_012.xhtml"
      },
      {
        "titleSuggest": "Kéž bych se mohl pomstít jedenkrát",
        "subtitleSuggest": "",
        "filename": "tri_lasky_013.md",
        "xhtml": "tri_lasky_013.xhtml"
      },
      {
        "titleSuggest": "Chci zemřít pro tvou krásu, paní má,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_014.md",
        "xhtml": "tri_lasky_014.xhtml"
      },
      {
        "titleSuggest": "Až budem spolu v chrámu stát",
        "subtitleSuggest": "",
        "filename": "tri_lasky_015.md",
        "xhtml": "tri_lasky_015.xhtml"
      },
      {
        "titleSuggest": "Ach, lásko, jak je krásná moje milá,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_016.md",
        "xhtml": "tri_lasky_016.xhtml"
      },
      {
        "titleSuggest": "Nebesa, vánky, pahorky a pláně,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_017.md",
        "xhtml": "tri_lasky_017.xhtml"
      },
      {
        "titleSuggest": "Zemřu-li ve tvé náruči, má paní,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_018.md",
        "xhtml": "tri_lasky_018.xhtml"
      },
      {
        "titleSuggest": "Přijmi tu růži, milou jako ty,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_019.md",
        "xhtml": "tri_lasky_019.xhtml"
      },
      {
        "titleSuggest": "Rozumu zbaven, vášní zotročen",
        "subtitleSuggest": "",
        "filename": "tri_lasky_020.md",
        "xhtml": "tri_lasky_020.xhtml"
      },
      {
        "titleSuggest": "Nemohu žít a přitom nevzdychat",
        "subtitleSuggest": "",
        "filename": "tri_lasky_021.md",
        "xhtml": "tri_lasky_021.xhtml"
      },
      {
        "titleSuggest": "Měsíc se do měsíce",
        "subtitleSuggest": "",
        "filename": "tri_lasky_022.md",
        "xhtml": "tri_lasky_022.xhtml"
      },
      {
        "titleSuggest": "Zděšený hledám, kde bych našel pramen",
        "subtitleSuggest": "",
        "filename": "tri_lasky_023.md",
        "xhtml": "tri_lasky_023.xhtml"
      },
      {
        "titleSuggest": "I teď, kdy nebe a kdy celá zem",
        "subtitleSuggest": "",
        "filename": "tri_lasky_024.md",
        "xhtml": "tri_lasky_024.xhtml"
      },
      {
        "titleSuggest": "Helena Trojská vykreslila nití",
        "subtitleSuggest": "",
        "filename": "tri_lasky_025.md",
        "xhtml": "tri_lasky_025.xhtml"
      },
      {
        "titleSuggest": "Když tě tak někdy vidím zadumanou",
        "subtitleSuggest": "",
        "filename": "tri_lasky_026.md",
        "xhtml": "tri_lasky_026.xhtml"
      },
      {
        "titleSuggest": "Od toho dne, co lásku znám,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_027.md",
        "xhtml": "tri_lasky_027.xhtml"
      },
      {
        "titleSuggest": "Chci tři dny jenom číst a přečíst Iliadu,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_028.md",
        "xhtml": "tri_lasky_028.xhtml"
      },
      {
        "titleSuggest": "Mám už hlavu jako trám,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_029.md",
        "xhtml": "tri_lasky_029.xhtml"
      },
      {
        "titleSuggest": "MARIE",
        "subtitleSuggest": "",
        "filename": "tri_lasky_030.md",
        "xhtml": "tri_lasky_030.xhtml"
      },
      {
        "titleSuggest": "Zde je mé srdce, zde je, moje paní,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_031.md",
        "xhtml": "tri_lasky_031.xhtml"
      },
      {
        "titleSuggest": "Kdo by chtěl, Marie, tvé jméno rýmovat,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_032.md",
        "xhtml": "tri_lasky_032.xhtml"
      },
      {
        "titleSuggest": "Láska je kouzelník: když přijdu ke své milé",
        "subtitleSuggest": "",
        "filename": "tri_lasky_033.md",
        "xhtml": "tri_lasky_033.xhtml"
      },
      {
        "titleSuggest": "Má milá je můj anděl z ráje,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_034.md",
        "xhtml": "tri_lasky_034.xhtml"
      },
      {
        "titleSuggest": "Pryč, srdce, honem pryč, ať už se neplahočí",
        "subtitleSuggest": "",
        "filename": "tri_lasky_035.md",
        "xhtml": "tri_lasky_035.xhtml"
      },
      {
        "titleSuggest": "Víc než má jaro květů v háji",
        "subtitleSuggest": "",
        "filename": "tri_lasky_036.md",
        "xhtml": "tri_lasky_036.xhtml"
      },
      {
        "titleSuggest": "Tu kytici, kterou vám posílám,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_037.md",
        "xhtml": "tri_lasky_037.xhtml"
      },
      {
        "titleSuggest": "Přitiskni, moje milá,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_038.md",
        "xhtml": "tri_lasky_038.xhtml"
      },
      {
        "titleSuggest": "Miláčku, vstaňte již, smí se tak dlouho spát?",
        "subtitleSuggest": "",
        "filename": "tri_lasky_039.md",
        "xhtml": "tri_lasky_039.xhtml"
      },
      {
        "titleSuggest": "Mlč, vlaštovko, ty repetilko,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_040.md",
        "xhtml": "tri_lasky_040.xhtml"
      },
      {
        "titleSuggest": "Slavíku, miláčku, co zpíváš ve vrboví",
        "subtitleSuggest": "",
        "filename": "tri_lasky_041.md",
        "xhtml": "tri_lasky_041.xhtml"
      },
      {
        "titleSuggest": "Ty se ptáš, moje znejmilejší,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_042.md",
        "xhtml": "tri_lasky_042.xhtml"
      },
      {
        "titleSuggest": "Klíč k mému srdci jste odnesla, moje paní,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_043.md",
        "xhtml": "tri_lasky_043.xhtml"
      },
      {
        "titleSuggest": "Já jsem tak vroucí milenec,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_044.md",
        "xhtml": "tri_lasky_044.xhtml"
      },
      {
        "titleSuggest": "Marie, polib mne; ale ne, nelíbej mě,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_045.md",
        "xhtml": "tri_lasky_045.xhtml"
      },
      {
        "titleSuggest": "Jak jen jí dokážu dát sbohem při loučení,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_046.md",
        "xhtml": "tri_lasky_046.xhtml"
      },
      {
        "titleSuggest": "Tak jako růži, jež rozkvétá na větvi,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_047.md",
        "xhtml": "tri_lasky_047.xhtml"
      },
      {
        "titleSuggest": "Má sladká mladost uplynula,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_048.md",
        "xhtml": "tri_lasky_048.xhtml"
      },
      {
        "titleSuggest": "HELENA",
        "subtitleSuggest": "",
        "filename": "tri_lasky_049.md",
        "xhtml": "tri_lasky_049.xhtml"
      },
      {
        "titleSuggest": "Přísahám, Heleno, dnes prvního dne v máji",
        "subtitleSuggest": "",
        "filename": "tri_lasky_050.md",
        "xhtml": "tri_lasky_050.xhtml"
      },
      {
        "titleSuggest": "Když piji dlouhými doušky ten sladký svit",
        "subtitleSuggest": "",
        "filename": "tri_lasky_051.md",
        "xhtml": "tri_lasky_051.xhtml"
      },
      {
        "titleSuggest": "Když jsem stál onehdy na schodech před domem",
        "subtitleSuggest": "",
        "filename": "tri_lasky_052.md",
        "xhtml": "tri_lasky_052.xhtml"
      },
      {
        "titleSuggest": "Tolikrát hněvat se, tolikrát smiřovat,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_053.md",
        "xhtml": "tri_lasky_053.xhtml"
      },
      {
        "titleSuggest": "Trnitý jalovče, jejž hostí horský svah,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_054.md",
        "xhtml": "tri_lasky_054.xhtml"
      },
      {
        "titleSuggest": "Objímej, líbej mě, svírej mě, moje paní,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_055.md",
        "xhtml": "tri_lasky_055.xhtml"
      },
      {
        "titleSuggest": "Jestli je láska snít a myslet ve dne v noci",
        "subtitleSuggest": "",
        "filename": "tri_lasky_056.md",
        "xhtml": "tri_lasky_056.xhtml"
      },
      {
        "titleSuggest": "Když jsem tu onehdy směl s vámi hovořit,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_057.md",
        "xhtml": "tri_lasky_057.xhtml"
      },
      {
        "titleSuggest": "Jestli je samý led, samý sníh celé roky,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_058.md",
        "xhtml": "tri_lasky_058.xhtml"
      },
      {
        "titleSuggest": "Vy tady tančíte ve víru ostatních",
        "subtitleSuggest": "",
        "filename": "tri_lasky_059.md",
        "xhtml": "tri_lasky_059.xhtml"
      },
      {
        "titleSuggest": "Sbohem, má Kasandro, i vy, má Marie,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_060.md",
        "xhtml": "tri_lasky_060.xhtml"
      },
      {
        "titleSuggest": "Když jsem dnes uléhal, viděl jsem hasnout svíci",
        "subtitleSuggest": "",
        "filename": "tri_lasky_061.md",
        "xhtml": "tri_lasky_061.xhtml"
      },
      {
        "titleSuggest": "Lásko, jež jediná máš klíč k mým myšlenkám,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_062.md",
        "xhtml": "tri_lasky_062.xhtml"
      },
      {
        "titleSuggest": "Komáre, obludo se sloním chobotem,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_063.md",
        "xhtml": "tri_lasky_063.xhtml"
      },
      {
        "titleSuggest": "Vyjíždět k indickým ostrovům za obchody",
        "subtitleSuggest": "",
        "filename": "tri_lasky_064.md",
        "xhtml": "tri_lasky_064.xhtml"
      },
      {
        "titleSuggest": "Není to bláznovství, o lásce psát si dnes,",
        "subtitleSuggest": "",
        "filename": "tri_lasky_065.md",
        "xhtml": "tri_lasky_065.xhtml"
      },
      {
        "titleSuggest": "Za dlouhých nočních chvil, kdy kočár luny krouží",
        "subtitleSuggest": "",
        "filename": "tri_lasky_066.md",
        "xhtml": "tri_lasky_066.xhtml"
      },
      {
        "titleSuggest": "Až jednou zestárlá budete v záři svíce",
        "subtitleSuggest": "",
        "filename": "tri_lasky_067.md",
        "xhtml": "tri_lasky_067.xhtml"
      },
      {
        "titleSuggest": "Sbohem, ty ukrutná, už jsem ti na obtíž;",
        "subtitleSuggest": "",
        "filename": "tri_lasky_068.md",
        "xhtml": "tri_lasky_068.xhtml"
      },
      {
        "titleSuggest": "DOSLOV",
        "subtitleSuggest": "",
        "filename": "tri_lasky_069.md",
        "xhtml": "tri_lasky_069.xhtml"
      },
      {
        "titleSuggest": "EDIČNÍ POZNÁMKA",
        "subtitleSuggest": "",
        "filename": "tri_lasky_070.md",
        "xhtml": "tri_lasky_070.xhtml"
      },
